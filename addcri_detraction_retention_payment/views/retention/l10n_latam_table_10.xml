<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="l10n_latam_table_10_view_form" model="ir.ui.view">
        <field name="name">l10n_latam.table.10.view.form</field>
        <field name="model">l10n_latam.table.10</field>
        <field name="arch" type="xml">
            <form string="Medios de pago" create="0" edit="0" delete="0">
                <sheet>
                    <group>
                        <group>
                            <field name="code" readonly="1"/>
                        </group>
                        <group>
                            <field name="name" readonly="1"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="l10n_latam_table_10_view_tree" model="ir.ui.view">
        <field name="name">l10n_latam.table.10.view.tree</field>
        <field name="model">l10n_latam.table.10</field>
        <field name="arch" type="xml">
            <tree string="Medios de pago" create="0" edit="0" delete="0">
                <field name="code"/>
                <field name="name"/>
            </tree>
        </field>
    </record>
    <record id="l10n_latam_table_10_action" model="ir.actions.act_window">
        <field name="name">Medios de pago</field>
        <field name="res_model">l10n_latam.table.10</field>
        <field name="view_mode">tree,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">Add a new record</p>
        </field>
    </record>
    <menuitem id="table_10_menu" name="Tabla 10 SUNAT" groups="account.group_account_manager" parent="account.menu_finance_configuration" sequence="10"/>
    <menuitem id="payment_methods_menu" name="Medios de pago" groups="account.group_account_manager" parent="table_10_menu" sequence="1" action="l10n_latam_table_10_action"/>
</odoo>