<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_move_form_inherit_not_domiciled" model="ir.ui.view">
        <field name="name">account.move.view.form.inherit</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook[1]" position="inside">
                <page name="not_domiciled" string="PLE - No domiciliado" attrs="{'invisible': ['|', ('move_type', '!=', 'in_invoice'), ('not_domiciled', '!=', True)]}">
                    <group>
                        <group>
                            <field name="sunat_table_25_id"/>
                            <field name="sunat_table_31_id"/>
                        </group>
                        <group>
                            <field name="not_domiciled_purchase_move_period"/>
                        </group>
                    </group>
                </page>
            </xpath>
        </field>
    </record>
</odoo>